{
  "name": "cursor-ai-memory",
  "version": "1.0.0",
  "description": "Lokal minneslösning för att förbättra AI-minne i Cursor med MCP-stöd",
  "main": "server.js",
  "bin": {
    "sam-cursor-memory": "./bin/cli.js"
  },
  "scripts": {
    "start": "node server.js",
    "start:proxy": "node cursor-integration.js",
    "start:mcp": "node mcp-server.js",
    "test": "node test-memory.js",
    "dev": "concurrently \"npm run start\" \"npm run start:proxy\"",
    "dev:mcp": "concurrently \"npm run start\" \"npm run start:mcp\"",
    "dev:all": "concurrently \"npm run start\" \"npm run start:proxy\" \"npm run start:mcp\"",
    "open-dashboard": "start http://localhost:3000/dashboard",
    "dashboard": "concurrently \"npm run start\" \"npm run start:proxy\" \"npm run start:mcp\" \"node -e \\\"setTimeout(() => require('child_process').exec('npm run open-dashboard'), 2000)\\\"\""
  },
  "keywords": [
    "ai",
    "memory",
    "cursor",
    "claude",
    "gemini",
    "mcp",
    "model-context-protocol"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "minimist": "^1.2.8",
    "natural": "^6.7.2",
    "os-utils": "^0.0.14"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "mcpSchema": {
    "version": "1.0.0",
    "supportsStreaming": false,
    "supportsKnowledgeBase": true,
    "supportsConversationMemory": true
  }
}
